            "identifier": "lookup_domain",
            "read_only": true,
            "parameters": {
                "domain": {
                    "description": "Domain to query",
                    "data_type": "string",
                    "order": 0,
                    "primary": true,
                    "required": true,
                    "contains": [
                        "domain"
                    ]
                },
                "type": {
                    "description": "Type of domain lookup",
                    "data_type": "string",
                    "order": 1,
                    "value_list": [
                        "mx",
                        "a",
                        "dns",
                        "spf",
                        "txt",
                        "soa"
                    ],
                    "required": true
                }
            },
            "output": [
                {
                    "data_path": "action_result.data.*.TTL",
                    "data_type": "string",
                    "example_values": [
                        "24 hrs"
                    ]
                },
                {
                    "data_path": "action_result.data.*.Status",
                    "data_type": "string",
                    "example_values": [
                        "[Green]"
